            <script runat="server">
                Platform.Load("Core", "1");
                var tsd_name = Request.GetFormField("Tsd_Name");

                if (tsd_name) {
                    var filter = {
                        Property: "Name",
                        SimpleOperator: "equals",
                        Value: tsd_name
                    };

                    var results = TriggeredSend.Retrieve(filter);

                    // Check if results are found and proceed
                    if (results && results.length > 0) {
                        Write("Found Trigger Send: " + tsd_name + "<br>");
                        Write(Stringify(results));

                        var tsd = TriggeredSend.Init(results[0]);
                        //var status = tsd.Publish();
                        Write("Publish Status: " + Stringify(status));
                    } else {
                        Write("No Trigger Send found with the name: " + tsd_name);
                    }
                } else {
                    Write("Please enter a Trigger Send name.");
                }
            </script>
